{
  "name": "tachijs",
  "version": "0.1.2",
  "description": "Highly testable dead simple web server written in Typescript",
  "main": "dist",
  "types": "types",
  "files": [
    "dist/**/*",
    "!dist/examples/**/*",
    "!dist/specs/**/*",
    "types/**/*",
    "!types/examples/**/*",
    "!types/specs/**/*"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "format": "prettier --write \"src/**/*\"",
    "test": "npm run test:types && npm run test:api-coverage",
    "test:api": "jest",
    "test:api-watch": "jest --watch",
    "test:api-coverage": "jest --coverage",
    "test:types-watch": "tsc --watch --noEmit",
    "test:types": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [],
  "author": "Junyoung Choi <fluke8259@gmail.com> (https://github.com/rokt33r)",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.15.2",
    "@types/pug": "^2.0.4",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.18.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.12",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.12"
  }
}
